{
  "manifest_version": 2,

  "name": "To Google",
  "description": "This extension repeats the current web search on Google",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background":
  {
    "scripts": ["background.js"]
  },
  
  "permissions": [
    "tabs","<all_urls>"
  ]
}